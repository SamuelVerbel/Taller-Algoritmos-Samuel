import java.util.Scanner;
import java.time.LocalDate;

public class EdadEtapa {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Ingrese el aÃ±o de nacimiento: ");
        int anoNacimiento = scanner.nextInt();
        
        int anoActual = LocalDate.now().getYear();
        int edad = anoActual - anoNacimiento;

        String etapa;

        if (edad >= 0 && edad <= 5) {
            etapa = "Primera infancia";
        } else if (edad <= 11) {
            etapa = "Infancia";
        } else if (edad <= 18) {
            etapa = "Adolescencia";
        } else if (edad <= 26) {
            etapa = "Joven";
        } else if (edad <= 59) {
            etapa = "Adultez";
        } else if (edad <= 70) {
            etapa = "Tercera edad";
        } else {
            etapa = "Adulto mayor";
        }

        System.out.println("La persona estÃ¡ en la etapa: " + etapa);
    }
}
